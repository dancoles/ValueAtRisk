import java.util.ArrayList;

public class portfolio {
	ArrayList<Stock> PF;
	int VaR;

	public portfolio() {
		PF = new ArrayList<Stock>();
		VaR = 0;
	}

	public ArrayList<Stock> getPF() {
		return this.PF;
	}
	public int getVar() {
		return this.VaR;
	}
	
	public boolean addToPF(Stock myStock) {
		return this.PF.add(myStock); //returns true if stock successfully added to PF
	}
	
	public void calcVaR(int confLevel, int period) {
		
	}
}
