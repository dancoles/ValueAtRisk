
public class Stock {
	String stockSymbol;
	float volatility;

	public Stock(String stockSymbol) {
		this.stockSymbol = stockSymbol;
		this.volatility = 0;
	}

	public String getSymbol() {
		return this.stockSymbol;
	}

	public float getVolatility() {
		return this.volatility;
	}	
	
	public float calcVolatility() {
		return 0;
	}
}
